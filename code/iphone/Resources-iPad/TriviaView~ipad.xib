                <label opaque="NO" clipsSubviews="YES" userInteractionEnabled="NO" contentMode="left" text="Trivia" textAlignment="center" lineBreakMode="tailTruncation" minimumFontSize="10" id="5" customClass="UIFontLabel">
                    <rect key="frame" x="435" y="20" width="159" height="77"/>
